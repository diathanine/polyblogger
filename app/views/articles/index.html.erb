<%= link_to 'create a new article!', new_article_path%>

<table>
  <% @articles.each do |article|%>
    <tr>
      <td><%=link_to article.title.to_s, article_path(article)%></td>
      <td>user: <%=link_to article.profile_id.to_s, profile_path(profile_by_id article.profile_id)%></td>
      <td><%= article.created_at%></td>
    </tr>
  <%end%>
</table>
